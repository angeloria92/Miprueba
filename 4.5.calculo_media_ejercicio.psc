Proceso CalcularMedia
	
	Definir num1,num2,num3,media Como Real;
	
	num1<-0;
	num2<-0;
	num3<-0;
	media<-0;
	Escribir " escribir numero ";
	Leer num1;
	
	Escribir " escribir numero ";
	Leer num2;
	
	Escribir " escribir numero ";
	Leer num3;
	
	media<-(num1+num2+num3)/3;
	Escribir " Este es el resultado: ",media;
FinProceso
